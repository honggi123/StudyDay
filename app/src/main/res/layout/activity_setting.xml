<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".ui.setting.SettingActivity">

        <include
            android:id="@+id/setting_toolbar"
            layout="@layout/main_toolbar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/setting_profile"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/under_line_gray"
            android:padding="18dp"
            android:text="프로필 관리"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/under_line_gray"
            android:padding="18dp"
            android:text="서비스 이용약관"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/under_line_gray"
            android:padding="18dp"
            android:text="개인정보처리방침"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/under_line_gray"
            android:padding="18dp"
            android:text="오픈 라이센스 정책"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/under_line_gray"
            android:padding="18dp"
            android:text="공지사항"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/setting_to_developer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/under_line_gray"
            android:padding="18dp"
            android:text="개발자에게 문의"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView112"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/under_line_gray"
            android:padding="18dp"
            android:text="앱 버전 1.0.1"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/setting_logout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/under_line_gray"
            android:padding="18dp"
            android:text="로그아웃"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/setting_withdrawal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/under_line_gray"
            android:padding="18dp"
            android:text="회원탈퇴"
            android:textColor="@color/black"
            android:textSize="18sp" />

    </LinearLayout>
</layout>